    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.1.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
